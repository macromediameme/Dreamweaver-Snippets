<?xml version="1.0" encoding="utf-8"?>
<snippet name = "Is Number Float" description = "Determines whether a given string is a floating-point number." preview="code" type="block">
<insertText location="beforeSelection">
<![CDATA[function isNumberFloat(inputString)
{
  return (!isNaN(parseFloat(inputString))) ? true : false;
}]]>
</insertText>
<insertText location="afterSelection"><![CDATA[]]>
</insertText>
</snippet>
