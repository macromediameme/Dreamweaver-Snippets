<snippet name = "No Numbers" description = "Verifies that a given string contains no numerals." preview="code" type="block">
<insertText location="beforeSelection">
<![CDATA[function onlyNumbers(inputString)
{
  var searchForNumbers = /\D+\_+\W+\s+\S+/
  return searchForNumbers.test(inputString);
} ]]>
</insertText>
<insertText location="afterSelection"><![CDATA[]]>
</insertText>
</snippet>
