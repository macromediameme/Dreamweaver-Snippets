<?xml version="1.0" encoding="utf-8"?>
<snippet name = "Seconds to Minutes" description = "Converts seconds to minutes, providing result as a string in the form 'minutes:seconds'." preview="code" type="block">
<insertText location="beforeSelection">
<![CDATA[function secondsToMinutes(sec)
{
  var min = Math.floor(sec/60);
  sec = sec % 60;
  if(sec<10) sec = "0" + sec;
  if(min<10) min = "0" + min;
  return min + ":" + sec;
}
]]>
</insertText>
<insertText location="afterSelection"><![CDATA[]]>
</insertText>
</snippet>
