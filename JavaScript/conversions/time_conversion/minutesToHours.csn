<?xml version="1.0" encoding="utf-8"?>
<snippet name = "Minutes to Hours" description = "Converts minutes to hours and returns a string "hrs:min"." preview="code" type="block">
<insertText location="beforeSelection">
<![CDATA[function minutesToHours(min)
{
  var hrs = Math.floor(min/60);
  min = min % 60;
  if(min<10) min = "0" + min;
  return hrs + ":" + min;
}
]]>
</insertText>
<insertText location="afterSelection"><![CDATA[]]>
</insertText>
</snippet>
